using System;

namespace Test1
{
  class Program
    {
        private static string strHelloWorld;

        static Program()
        {
            strHelloWorld = "Hello World";
        }
        void writeHelloWorld()
        {
            Console.WriteLine(strHelloWorld);
        }

        public static void Main()
        {
            Program hw = new Program();
            hw.writeHelloWorld();
            Console.ReadKey();
        }
    }
}
