import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {
		
		
		
      Scanner str = new Scanner(System.in);
	  int n = str.nextInt();
      int i =1;
      int a = 65;
      
      while (i<=n) {
        int j =1;
        
        char b = (char)a;
        while (j<=i) {
          System.out.print(b);
          
          j=j+1;

        }
                  a=a+1;
        i=i+1;
        System.out.println();
        
      }
		
	}

}
